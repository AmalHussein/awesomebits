<%= simple_form_for user do |form| %>
  <%= hidden_field_tag :return_to, params[:return_to] %>
  <%= form.input :first_name %>
  <%= form.input :last_name %>
  <%= form.input :email %>
  <%= form.input :url %>
  <%= form.input :new_password, :as => :password, :label => t(:'simple_form.labels.user.new_password') %>
  <%= form.button :submit %>
<% end %>
